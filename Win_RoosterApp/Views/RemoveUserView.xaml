<Window x:Class="Win_RoosterApp.rmUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Win_RoosterApp"
        mc:Ignorable="d"
        Title="Verwijder een gebruiker" Height="110" Width="268" ShowInTaskbar="False" Topmost="True" WindowStyle="ToolWindow">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="3*"></RowDefinition>
            <RowDefinition Height="1*"></RowDefinition>
        </Grid.RowDefinitions>
        <TextBox x:Name="ioRemoveUser" Grid.Row="0" Text=""></TextBox>
        <Button Click="RemoveUser_Click" Grid.Row="1" VerticalAlignment="Center" FontFamily="24px" FontWeight="Bold" HorizontalAlignment="Center" Content="Oke"></Button>
    </Grid>
</Window>
